{
  "name": "regan-astro",
  "type": "module",
  "version": "0.0.1",
  "main": "dist/regan-astro.js",
  "types": "dist/types/",
  "scripts": {
    "dev": "astro dev",
    "build": "bun build ./src/regan-astro.ts --outdir ./dist",
    "build-client": "bun build ./src/client.ts --external regan --outdir ./dist",
    "build-server": "bun build ./src/server.ts --external regan --external astro --outdir ./dist",
    "types": "tsc --project tsconfig.gen.json",
    "astro": "astro"
  },
  "exports": {
    ".": "./dist/regan-astro.js",
    "./client.js": "./dist/client.js",
    "./client-dev.js": "./dist/client.js",
    "./server.js": "./dist/server.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "astro": "^5.6.1",
    "regan": "*",
    "regan-vite": "*"
  }
}
